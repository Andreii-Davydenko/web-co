
@{
    ViewData["Title"] = "Index";
}

<h1>Converters</h1>

<div class="row">
    <div class="col">
        
        @using (Html.BeginForm("CsvToXlsx", "Convert", FormMethod.Post, new { enctype= "multipart/form-data" }))
        {
            @:Csv => Xlsx <input type="file" name="file" />

            <button type="submit" class="btn btn-primary">Convert</button>
        }
    </div>
</div>

